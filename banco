create database jogo;

use jogo;

CREATE TABLE score (
tag varchar(255),
points int 
);

insert into score (tag, points) values ('abc', 8);
insert into score (tag, points) values ('abcde', 10);
insert into score (tag, points) values ('abcdefg', 12);

select * from score;

SELECT * FROM score ORDER BY points DESC LIMIT 5;